{
  "dependencies": {
    "@nestjs/mapped-types": "*"
  },
  "name": "app",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "app:develop": "cd packages/app/ && npm start",
    "backend:develop": "cd packages/backend/ && npm develop",
    "backend:nest:develop": "cd packages/backend-nest/ && npm run start:dev",
    "app:release": "cd packages/app/ && expo build:android --release-channel staging"
  }
}
